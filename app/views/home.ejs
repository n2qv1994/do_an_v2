<script type="text/javascript">
var room_name = "";
var room_link = "";
  $(".join_room").on("click", function() {
    $("#myModal").modal('show');
    room_name = $(this).children('.room_name').text();
    room_link = $(this).children('.room_link').text();
  });

  $('#login_form').submit(function(){ //listen for submit event
    var name = $("<input />")
                    .attr('type', 'hidden')
                    .attr('name', "room_name")
                    .attr('value', room_name);
    var link = $("<input />")
                    .attr('type', 'hidden')
                    .attr('name', "room_link")
                    .attr('value', room_link);           
    $('#login_form').append(name);
    $('#login_form').append(link);
    return true;
}); 
</script>